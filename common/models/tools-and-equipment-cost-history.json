{
  "name": "ToolsAndEquipmentCostHistory",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "scope": {
    "order": "createdAt DESC"
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "required": true
    },
    "cost": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 10,
      "scale": 4,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "material": {
      "type": "belongsTo",
      "model": "ToolsAndEquipment",
      "foreignKey": ""
    },
    "region": {
      "type": "belongsTo",
      "model": "Region",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
