{
  "name": "Material",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "code": {
      "type": "number",
      "required": true,
      "length": 10,
      "mysql": {
        "dataType": "INT",
        "dataLength": 10,
        "nullable": "N"
      }
    },
    "description": {
      "type": "string",
      "required": true
    },
    "waste": {
      "type": "number",
      "required": true,
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "entity": {
      "type": "belongsTo",
      "model": "Entity",
      "foreignKey": ""
    },
    "unitsOfMeasurement": {
      "type": "belongsTo",
      "model": "UnitsOfMeasurement",
      "foreignKey": ""
    },
    "materialCostHistories": {
      "type": "hasMany",
      "model": "MaterialCostHistory",
      "foreignKey": ""
    },
    "costSheets": {
      "type": "hasMany",
      "model": "CostSheet",
      "foreignKey": "",
      "through": "CostSheetHasMaterials"
    }
  },
  "acls": [],
  "methods": {}
}
