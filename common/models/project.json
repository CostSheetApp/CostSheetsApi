{
  "name": "Project",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "budget": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 18,
      "scale": 4,
      "default": 0
    },
    "cost": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 18,
      "scale": 4,
      "default": 0
    },
    "profitPercentage": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 10,
      "scale": 4,
      "default": 0
    },
    "isDeleted": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "costSheets": {
      "type": "hasMany",
      "model": "CostSheet",
      "foreignKey": "",
      "through": "ProjectHasCostSheets"
    },
    "entities": {
      "type": "hasMany",
      "model": "Entity",
      "foreignKey": "",
      "through": "EntityHasProjects"
    }
  },
  "acls": [],
  "methods": {}
}
