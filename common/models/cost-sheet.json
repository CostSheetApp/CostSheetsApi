{
  "name": "CostSheet",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "minimumUnit": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "minimumCost": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "description": {
      "type": "string",
      "required": true,
      "default": "new cost sheet description"
    }
  },
  "validations": [],
  "relations": {
    "entities": {
      "type": "hasMany",
      "model": "Entity",
      "foreignKey": "",
      "through": "EntityHasCostSheets"
    },
    "projects": {
      "type": "hasMany",
      "model": "Project",
      "foreignKey": "",
      "through": "ProjectHasCostSheets"
    },
    "region": {
      "type": "belongsTo",
      "model": "Region",
      "foreignKey": ""
    },
    "unitsOfMeasurement": {
      "type": "belongsTo",
      "model": "UnitsOfMeasurement",
      "foreignKey": ""
    },
    "materials": {
      "type": "hasMany",
      "model": "Material",
      "foreignKey": "",
      "through": "CostSheetHasMaterial"
    },
    "manpowers": {
      "type": "hasMany",
      "model": "Manpower",
      "foreignKey": "",
      "through": "CostSheetHasManpower"
    },
    "toolsAndEquipments": {
      "type": "hasMany",
      "model": "ToolsAndEquipment",
      "foreignKey": "",
      "through": "CostSheetHasToolsAndEquipment"
    }
  },
  "acls": [],
  "methods": {}
}
